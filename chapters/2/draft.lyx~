\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
Rewriting Diagrams
\end_layout

\end_inset


\begin_inset CommandInset label
LatexCommand label
name "fig:shape-diagrams"

\end_inset


